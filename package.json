{
  "name": "tanatloc-electron",
  "description": "Tanatloc",
  "version": "1.0.0",
  "repository": "git@github.com:Airthium/tanatloc-electron.git",
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "scripts": {
    "build": "yarn prebuild && yarn serverbuild && nextron build && electron-builder install-all-deps",
    "prebuild": "yarn prebuildconfig && yarn prebuildinstall && yarn prebuilddatabase && yarn prebuildrun",
    "prebuildconfig": "swc --config-file .swcrc tanatloc-ssr/config --out-dir dist-install",
    "prebuildinstall": "swc --config-file .swcrc tanatloc-ssr/install --out-dir dist-install",
    "prebuilddatabase": "swc --config-file .swcrc tanatloc-ssr/src/database --out-dir dist-install",
    "prebuildrun": "node dist-install/install",
    "serverbuild": "yarn serverbuildconfig && yarn serverbuildmodels && yarn serverbuildmodules && yarn serverbuildplugins && yarn serverbuildserver && yarn serverbuildauth && yarn serverbuilddatabase && yarn serverbuildlib && yarn serverbuildroute && yarn serverbuildservices && yarn serverbuildtemplates",
    "serverbuildconfig": "swc --config-file .swcrc tanatloc-ssr/config --out-dir dist-server/tanatloc-ssr",
    "serverbuildmodels": "swc --config-file .swcrc tanatloc-ssr/models --out-dir dist-server/tanatloc-ssr",
    "serverbuildmodules": "swc --config-file .swcrc tanatloc-ssr/modules --out-dir dist-server/tanatloc-ssr",
    "serverbuildplugins": "swc --config-file .swcrc tanatloc-ssr/plugins --out-dir dist-server/tanatloc-ssr",
    "serverbuildserver": "swc server --out-dir dist-server/server",
    "serverbuildauth": "swc --config-file .swcrc tanatloc-ssr/src/auth --out-dir dist-server/tanatloc-ssr",
    "serverbuilddatabase": "swc --config-file .swcrc tanatloc-ssr/src/database --out-dir dist-server/tanatloc-ssr",
    "serverbuildlib": "swc --config-file .swcrc tanatloc-ssr/src/lib --out-dir dist-server/tanatloc-ssr",
    "serverbuildroute": "swc --config-file .swcrc tanatloc-ssr/src/route --out-dir dist-server/tanatloc-ssr",
    "serverbuildservices": "swc --config-file .swcrc tanatloc-ssr/src/services --out-dir dist-server/tanatloc-ssr",
    "serverbuildtemplates": "swc --config-file .swcrc tanatloc-ssr/templates --out-dir dist-server/tanatloc-ssr",
    "depcheck": "depcheck",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "test": "jest --coverage --silent"
  },
  "dependencies": {
    "electron-serve": "1.1.0",
    "electron-store": "8.0.1"
  },
  "devDependencies": {
    "@ant-design/icons": "4.7.0",
    "@hapi/iron": "6.0.0",
    "@reduxjs/toolkit": "1.6.2",
    "@sentry/browser": "6.15.0",
    "@sentry/node": "6.15.0",
    "@swc/cli": "0.1.52",
    "@swc/core": "1.2.117",
    "@swc/jest": "0.2.11",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.3",
    "antd": "4.17.2",
    "archiver": "5.3.0",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "better-react-mathjax": "1.0.3",
    "cookie": "0.4.1",
    "cors": "2.8.5",
    "deep-object-diff": "1.1.0",
    "depcheck": "1.4.2",
    "ejs": "3.1.6",
    "electron": "16.0.3",
    "electron-builder": "22.14.5",
    "eslint": "8.3.0",
    "eslint-config-next": "12.0.4",
    "express": "4.17.1",
    "http-errors": "1.8.1",
    "ignore-loader": "0.1.2",
    "is-docker": "3.0.0",
    "is-electron": "2.2.1",
    "jest": "27.4.3",
    "jest-canvas-mock": "2.3.1",
    "lodash.merge": "4.6.2",
    "mailersend": "1.3.0",
    "next": "12.0.4",
    "next-compose-plugins": "2.2.1",
    "next-plugin-antd-less": "1.5.2",
    "nextron": "7.1.0",
    "node-fetch": "3.1.0",
    "passport": "0.5.0",
    "passport-local": "1.0.0",
    "pg": "8.7.1",
    "pg-native": "./tanatloc-ssr/modules/pg-native",
    "prettier": "2.5.0",
    "prism-themes": "1.9.0",
    "prismjs": "1.25.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-simple-code-editor": "0.11.0",
    "recharts": "2.1.6",
    "redux": "4.1.2",
    "redux-devtools-extension": "2.13.9",
    "redux-persist": "6.0.0",
    "set-interval-async": "2.0.3",
    "swr": "1.1.0",
    "tar": "6.1.11",
    "three": "0.135.0",
    "three-to-glb": "./tanatloc-ssr/modules/three-to-glb/",
    "typedoc": "0.22.10",
    "typescript": "4.5.2",
    "url-join": "4.0.1",
    "uuid": "8.3.2"
  }
}
