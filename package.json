{
  "name": "tanatloc-electron",
  "version": "1.0.0",
  "private": true,
  "description": "Tanatloc",
  "repository": "git@github.com:Airthium/tanatloc-electron.git",
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "workspaces": [
    "tanatloc"
  ],
  "scripts": {
    "distclean": "rm -Rf app dist dist-install dist-server plugins",
    "depcheck": "node config/depcheckrc.js",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "test": "jest --config config/jest.config.js --coverage --silent",
    "doc": "typedoc --options config/typedoc.config.json",
    "sync-deps": "node config/syncDeps.js",
    "prebuild": "yarn distclean && yarn prebuild:install && yarn prebuild:server && yarn prebuild:plugins && yarn prebuild:postprocessing",
    "prebuild:install": "echo '- Install' && yarn prebuild:install:config && yarn prebuild:install:database && yarn prebuild:install:init && yarn prebuild:install:install",
    "prebuild:install:config": "babel tanatloc/config/db.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/config && babel tanatloc/config/storage.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/config",
    "prebuild:install:database": "babel tanatloc/src/database/index.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/src/database",
    "prebuild:install:init": "babel tanatloc/src/server/init/database.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/src/server/init",
    "prebuild:install:install": "babel tanatloc/install/*.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/install",
    "prebuild:install:run": "node dist-install/install",
    "prebuild:server": "echo '- Server' && yarn prebuild:server:server && yarn prebuild:server:config && yarn prebuild:server:src && yarn prebuild:server:models && yarn prebuild:server:plugins && yarn prebuild:server:templates",
    "prebuild:server:server": "babel server --extensions .ts --config-file ./config/babelrc --out-dir dist-server/server --ignore '**/__tests__/**'",
    "prebuild:server:config": "babel tanatloc/config --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/config --ignore '**/__tests__/**'",
    "prebuild:server:src": "yarn prebuild:server:src:auth && yarn prebuild:server:src:database && yarn prebuild:server:src:lib && yarn prebuild:server:src:route && yarn prebuild:server:src:server && yarn prebuild:server:src:services",
    "prebuild:server:src:auth": "babel tanatloc/src/auth --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/src/auth --ignore '**/__tests__/**'",
    "prebuild:server:src:database": "babel tanatloc/src/database --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/src/database --ignore '**/__tests__/**'",
    "prebuild:server:src:lib": "babel tanatloc/src/lib --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/src/lib --ignore '**/__tests__/**'",
    "prebuild:server:src:route": "babel tanatloc/src/route --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/src/route --ignore '**/__tests__/**'",
    "prebuild:server:src:server": "babel tanatloc/src/server --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/src/server --ignore '**/__tests__/**'",
    "prebuild:server:src:services": "babel tanatloc/src/services --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/src/services --ignore '**/__tests__/**'",
    "prebuild:server:models": "babel tanatloc/models --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/models --ignore '**/__tests__/**'",
    "prebuild:server:plugins": "babel tanatloc/plugins --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/plugins  --ignore '**/__tests__/**'",
    "prebuild:server:templates": "babel tanatloc/templates --extensions .ts --config-file ./config/babelrc --out-dir dist-server/tanatloc/templates  --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "prebuild:plugins": "echo '- Plugins' && babel tanatloc/plugins --config-file ./config/babelrc.plugins --extensions .ts,.tsx --out-dir plugins --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "prebuild:postprocessing": "echo '- Postprocessing' && babel tanatloc/postprocessing --config-file ./config/babelrc.plugins --extensions .ts,.tsx --out-dir postprocessing --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "dev": "yarn nextron",
    "build": "echo '- Build' && yarn next telemetry disable && NEXT_PUBLIC_SOURCE_BRANCH=$(git submodule foreach --quiet 'git rev-parse --abbrev-ref HEAD') NEXT_PUBLIC_SOURCE_COMMIT=$(git submodule foreach --quiet 'git rev-parse --short HEAD') nextron build && electron-builder install-app-deps",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron-serve": "1.1.0",
    "electron-store": "8.1.0",
    "sharp": "0.31.0"
  },
  "devDependencies": {
    "@ant-design/icons": "4.7.0",
    "@babel/cli": "7.18.10",
    "@babel/core": "7.19.0",
    "@babel/preset-typescript": "7.18.6",
    "@hapi/iron": "7.0.0",
    "@sentry/browser": "7.12.1",
    "@sentry/node": "7.12.1",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.0",
    "@testing-library/react": "13.4.0",
    "@types/archiver": "5.3.1",
    "@types/cookie": "0.5.1",
    "@types/cors": "2.8.12",
    "@types/ejs": "3.1.1",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.2",
    "@types/jest": "29.0.1",
    "@types/lodash": "4.14.185",
    "@types/node": "17.0.2",
    "@types/passport": "1.0.10",
    "@types/passport-local": "1.0.34",
    "@types/pg": "8.6.5",
    "@types/react": "18.0.19",
    "@types/set-interval-async": "1.0.0",
    "@types/sharp": "0.30.5",
    "@types/tar": "6.1.2",
    "@types/three": "0.144.0",
    "@types/url-join": "4.0.1",
    "@types/uuid": "8.3.4",
    "antd": "4.23.1",
    "archiver": "5.3.1",
    "babel-jest": "29.0.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-preset-minify": "0.5.2",
    "cookie": "0.5.0",
    "cors": "2.8.5",
    "deep-object-diff": "1.1.7",
    "depcheck": "1.4.3",
    "ejs": "3.1.8",
    "electron": "20.1.3",
    "electron-builder": "23.3.3",
    "email-addresses": "5.0.0",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "express": "4.18.1",
    "extract-json-from-string": "1.0.1",
    "fix-path": "4.0.0",
    "form-data": "4.0.0",
    "html-react-parser": "3.0.4",
    "http-errors": "2.0.0",
    "https-proxy-agent": "5.0.1",
    "ignore-loader": "0.1.2",
    "is-docker": "2.2.1",
    "is-electron": "2.2.1",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "lodash": "4.17.21",
    "mailersend": "1.4.1",
    "mathjax": "3.2.2",
    "next": "12.3.0",
    "next-plugin-antd-less": "1.8.0",
    "next-transpile-modules": "9.0.0",
    "nextron": "8.2.0",
    "node-fetch": "3.2.10",
    "nodemon": "2.0.19",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "pg": "8.8.0",
    "pg-format": "https://github.com/Airthium/pg-format.git#1.0.8",
    "pg-native": "https://github.com/Airthium/pg-native.git#3.0.1",
    "prettier": "2.7.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "2.1.12",
    "set-interval-async": "3.0.2",
    "swr": "1.3.0",
    "tar": "6.1.11",
    "three": "0.144.0",
    "typedoc": "0.23.14",
    "typedoc-plugin-airthium": "https://github.com/Airthium/typedoc-plugin-airthium.git#1.0.6",
    "typescript": "4.8.3",
    "url-join": "4.0.1",
    "uuid": "9.0.0"
  },
  "packageManager": "yarn@3.2.3"
}
