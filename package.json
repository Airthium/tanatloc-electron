{
  "name": "tanatloc-electron",
  "version": "1.0.0",
  "private": true,
  "description": "Tanatloc",
  "repository": "git@github.com:Airthium/tanatloc-electron.git",
  "license": "AGPL-3.0-or-later",
  "main": "main/index.js",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "scripts": {
    "clean": "rm -rf dist dist-install dist-server main renderer",
    "depcheck": "node config/depcheck.js",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "test": "jest --config config/jest.config.js  --coverage --silent",
    "doc": "typedoc --options config/typedoc.config.json",
    "pre-build": "node esm_commonjs/index.js",
    "build-renderer": "echo Build renderer && NEXT_PUBLIC_SERVER_MODE=server next build tanatloc",
    "build-electron": "echo Build electron && tsc -p electron-src",
    "build-install": "echo Build install && yarn build-install:config && yarn build-install:database && yarn build-install:init && yarn build-install:package && yarn build-install:install",
    "build-install:config": "echo - Config && babel tanatloc/config/db.ts --extensions .ts --config-file ./install/.babelrc --out-dir dist-install/config && babel tanatloc/config/storage.ts --extensions .ts --config-file ./install/.babelrc --out-dir dist-install/config",
    "build-install:database": "echo - Database && babel tanatloc/src/database/index.ts --extensions .ts --config-file ./install/.babelrc --out-dir dist-install/src/database",
    "build-install:init": "echo - Init && babel tanatloc/src/server/init/database.ts --extensions .ts --config-file ./install/.babelrc --out-dir dist-install/src/server/init",
    "build-install:package": "echo - Package.json && node install/package.js",
    "build-install:install": "echo - Install && babel tanatloc/install --extensions .ts --config-file ./install/.babelrc --out-dir dist-install/install --ignore '**/__tests__/**'",
    "build-server": "echo Build server && yarn build-server:server && yarn build-server:config && yarn build-server:src && yarn build-server:models && yarn build-server:plugins && yarn build-server:mathjax && yarn build-server:templates && yarn build-server:postprocessing",
    "build-server:server": "echo - Server && babel server --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/server --ignore '**/__tests__/**'",
    "build-server:config": "echo - Config && babel tanatloc/config/*.ts --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/config",
    "build-server:src": "yarn build-server:src:auth && yarn build-server:src:database && yarn build-server:src:lib && yarn build-server:src:route && yarn build-server:src:server && yarn build-server:src:services",
    "build-server:src:auth": "echo - Src/auth && babel tanatloc/src/auth --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/src/auth --ignore '**/__tests__/**'",
    "build-server:src:database": "echo - Src/database && babel tanatloc/src/database --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/src/database --ignore '**/__tests__/**'",
    "build-server:src:lib": "echo - Src/lib && babel tanatloc/src/lib --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/src/lib --ignore '**/__tests__/**'",
    "build-server:src:route": "echo - Src/route && babel tanatloc/src/route --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/src/route --ignore '**/__tests__/**'",
    "build-server:src:server": "echo - Src/server && babel tanatloc/src/server --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/src/server --ignore '**/__tests__/**'",
    "build-server:src:services": "echo - Src/services && babel tanatloc/src/services --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/src/services --ignore '**/__tests__/**'",
    "build-server:models": "echo - Src/models && babel tanatloc/models --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/models --ignore '**/__tests__/**'",
    "build-server:templates": "echo - Src/templates && babel tanatloc/templates --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/templates --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "build-server:plugins": "echo - Src/plugins && babel tanatloc/plugins --extensions .ts,.tsx --config-file ./server/.babelrc --out-dir dist-server/tanatloc/plugins --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "build-server:mathjax": "echo - Src/components/assets/mathjax && babel tanatloc/src/components/assets/mathjax --extensions .ts,.tsx --config-file ./server/.babelrc --out-dir dist-server/tanatloc/src/components/assets/mathjax --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "build-server:postprocessing": "echo - Src/postprocessing && babel tanatloc/postprocessing --extensions .ts --config-file ./server/.babelrc --out-dir dist-server/tanatloc/postprocessing --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "build": "yarn pre-build && yarn build-install && yarn build-server && yarn build-renderer && yarn build-electron",
    "dist": "yarn build && echo Build application && electron-builder"
  },
  "dependencies": {
    "@airthium/extract-json-from-string": "1.0.40",
    "@airthium/pg-format": "2.0.45",
    "@airthium/tanatloc-formula-validator": "1.0.24",
    "@babel/runtime": "7.23.5",
    "@hapi/iron": "7.0.1",
    "@sentry/electron": "4.15.1",
    "@sentry/nextjs": "7.86.0",
    "archiver": "6.0.1",
    "cors": "2.8.5",
    "ejs": "3.1.9",
    "electron-serve": "1.2.0",
    "electron-store": "8.1.0",
    "express": "4.18.2",
    "fix-path": "4.0.0",
    "form-data": "4.0.0",
    "http-errors": "2.0.0",
    "https-proxy-agent": "7.0.2",
    "is-docker": "3.0.0",
    "is-electron": "2.2.2",
    "lodash": "4.17.21",
    "mailersend": "2.2.0",
    "node-fetch": "3.3.2",
    "passport": "0.7.0",
    "passport-local": "1.0.0",
    "pg": "8.11.3",
    "react-cookie": "6.1.1",
    "set-interval-async": "3.0.3",
    "sharp": "0.33.0",
    "tar": "6.2.0",
    "url-join": "5.0.0",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@airthium/deep-object-diff": "1.0.22",
    "@airthium/tanatloc-3d": "1.1.0",
    "@airthium/typedoc-plugin-airthium": "1.1.26",
    "@ant-design/icons": "5.2.6",
    "@babel/cli": "7.23.4",
    "@babel/preset-typescript": "7.23.3",
    "@swc/core": "1.3.100",
    "@testing-library/react": "14.1.2",
    "@types/archiver": "6.0.2",
    "@types/cookie": "0.6.0",
    "@types/cors": "2.8.17",
    "@types/ejs": "3.1.5",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/jest": "29.5.11",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.4",
    "@types/passport": "1.0.16",
    "@types/passport-local": "1.0.38",
    "@types/pg": "8.10.9",
    "@types/react": "18.2.43",
    "@types/set-interval-async": "1.0.3",
    "@types/sharp": "0.31.1",
    "@types/tar": "6.1.10",
    "@types/three": "0.159.0",
    "@types/url-join": "4.0.3",
    "@types/uuid": "9.0.7",
    "ajv": "8.12.0",
    "antd": "5.12.1",
    "babel-plugin-module-resolver": "5.0.0",
    "cookie": "0.6.0",
    "depcheck": "1.4.7",
    "electron": "28.0.0",
    "electron-builder": "24.9.1",
    "email-addresses": "5.0.0",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.4",
    "html-react-parser": "5.0.7",
    "ignore-loader": "0.1.2",
    "jest": "29.7.0",
    "mathjax": "3.2.2",
    "next": "14.0.2",
    "pg-native": "npm:@airthium/pg-native@3.0.2",
    "prettier": "3.1.1",
    "react": "18.2.0",
    "react-ace": "10.1.0",
    "react-dom": "18.2.0",
    "recharts": "2.10.3",
    "swr": "2.2.4",
    "three": "0.159.0",
    "typedoc": "0.25.4",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@4.0.2"
}
