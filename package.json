{
  "name": "tanatloc-electron",
  "version": "1.0.0",
  "private": true,
  "description": "Tanatloc",
  "repository": "git@github.com:Airthium/tanatloc-electron.git",
  "license": "UNLICENSED",
  "author": {
    "name": "Airthium",
    "url": "https://github.com/orgs/Airthium/people"
  },
  "main": "app/background.js",
  "workspaces": [
    "tanatloc/*"
  ],
  "scripts": {
    "prebuild": "yarn distclean && yarn prebuildinstall && yarn server && yarn plugins",
    "build": "echo '- Build' && yarn next telemetry disable && NEXT_PUBLIC_SOURCE_BRANCH=$(git submodule foreach --quiet 'git rev-parse --abbrev-ref HEAD') NEXT_PUBLIC_SOURCE_COMMIT=$(git submodule foreach --quiet 'git rev-parse --short HEAD') nextron build && electron-builder install-app-deps",
    "depcheck": "node config/depcheckrc.js",
    "dev": "yarn prebuild && yarn nextron",
    "distclean": "rm -Rf app dist dist-install dist-server plugins",
    "plugins": "echo '- Plugins' && babel tanatloc/plugins --config-file ./config/babelrc.plugins --extensions .js,.jsx,.ts,.tsx --out-dir plugins --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "prebuildinstall": "echo '- Install' && yarn prebuildinstallconfig && yarn prebuildinstallinstall && yarn prebuildinstalldatabase && yarn prebuildinstallinit && yarn prebuildinstalllib",
    "prebuildinstallconfig": "babel tanatloc/config/db.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/config && babel tanatloc/config/storage.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/config",
    "prebuildinstalldatabase": "babel tanatloc/src/database/index.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/src/database",
    "prebuildinstallinit": "babel tanatloc/src/init/index.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/src/init",
    "prebuildinstalllib": "babel mock/index.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/src/lib/plugins && babel mock/index.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/src/lib/template",
    "prebuildinstallinstall": "babel tanatloc/install/*.ts --extensions .ts --config-file ./config/babelrc --out-dir dist-install/install",
    "prebuildinstallrun": "node dist-install/install",
    "prettier": "prettier -l '**/*.{js,jsx,ts,tsx,json,md,css}'",
    "server": "echo '- Server' && yarn serverconfig && yarn servermodels && yarn serverplugins && yarn serverserver && yarn serverauth && yarn serverdatabase && yarn serverlib && yarn serverinit && yarn serverroute && yarn serverservices && yarn servertemplates",
    "serverauth": "babel tanatloc/src/auth --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/src/auth --ignore '**/__tests__/**'",
    "serverconfig": "babel tanatloc/config --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/config --ignore '**/__tests__/**' --ignore '**/depcheck/**' --ignore '**/jest/**'",
    "serverdatabase": "babel tanatloc/src/database --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/src/database --ignore '**/__tests__/**'",
    "serverlib": "babel tanatloc/src/lib --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/src/lib --ignore '**/__tests__/**'",
    "serverinit": "babel tanatloc/src/init --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/src/init --ignore '**/__tests__/**'",
    "servermodels": "babel tanatloc/models --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/models --ignore '**/__tests__/**'",
    "serverplugins": "babel tanatloc/plugins --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/plugins --ignore '**/__tests__/**'",
    "serverroute": "babel tanatloc/src/route --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/src/route --ignore '**/__tests__/**'",
    "serverrun": "node dist-server/server/bin/www",
    "serverserver": "babel server --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/server --ignore '**/__tests__/**'",
    "serverservices": "babel tanatloc/src/services --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/src/services --ignore '**/__tests__/**'",
    "servertemplates": "babel tanatloc/templates --config-file ./config/babelrc --extensions .js,.ts --out-dir dist-server/tanatloc/templates --ignore '**/__tests__/**' --copy-files --no-copy-ignored",
    "test": "jest --config config/jest.config.js --coverage --silent",
    "workflow": ".github/workflows/node.local.sh"
  },
  "dependencies": {
    "electron-serve": "1.1.0",
    "electron-store": "8.0.1",
    "sharp": "0.30.4",
    "three-to-glb": "ssh://git@github.com:Airthium/three-to-glb.git#1.2.2"
  },
  "devDependencies": {
    "@ant-design/icons": "4.7.0",
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@babel/preset-typescript": "7.16.7",
    "@hapi/iron": "6.0.0",
    "@sentry/browser": "6.19.6",
    "@sentry/node": "6.19.6",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.171",
    "@swc/jest": "0.2.20",
    "@testing-library/react": "13.1.1",
    "@types/archiver": "5.3.1",
    "@types/cookie": "0.5.1",
    "@types/cors": "2.8.12",
    "@types/ejs": "3.1.0",
    "@types/express": "4.17.13",
    "@types/http-errors": "1.8.2",
    "@types/jest": "27.4.1",
    "@types/passport": "1.0.7",
    "@types/passport-local": "1.0.34",
    "@types/pg": "8.6.5",
    "@types/pg-format": "1.0.2",
    "@types/prismjs": "1.26.0",
    "@types/set-interval-async": "1.0.0",
    "@types/tar": "6.1.1",
    "@types/three": "0.139.0",
    "@types/url-join": "4.0.1",
    "@types/uuid": "8.3.4",
    "antd": "4.20.0",
    "archiver": "5.3.1",
    "babel-jest": "27.5.1",
    "babel-plugin-module-resolver": "4.1.0",
    "cookie": "0.5.0",
    "cors": "2.8.5",
    "deep-object-diff": "1.1.7",
    "depcheck": "1.4.3",
    "ejs": "3.1.7",
    "electron": "18.1.0",
    "electron-builder": "23.0.3",
    "email-addresses": "5.0.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "express": "4.17.3",
    "form-data": "4.0.0",
    "html-react-parser": "1.4.12",
    "http-errors": "2.0.0",
    "https-proxy-agent": "5.0.1",
    "ignore-loader": "0.1.2",
    "is-docker": "2.2.1",
    "is-electron": "2.2.1",
    "jest": "27.5.1",
    "jest-canvas-mock": "2.3.1",
    "lodash": "4.17.21",
    "mailersend": "1.3.2",
    "mathjax": "3.2.0",
    "next": "12.1.5",
    "next-compose-plugins": "2.2.1",
    "next-plugin-antd-less": "1.8.0",
    "nextron": "8.0.0",
    "node-fetch": "3.2.3",
    "passport": "0.5.2",
    "passport-local": "1.0.0",
    "pg": "8.7.3",
    "pg-format": "ssh://git@github.com:Airthium/pg-format.git#1.0.6",
    "pg-native": "ssh://git@github.com:Airthium/pg-native.git#3.0.0",
    "prettier": "2.6.2",
    "prop-types": "15.8.1",
    "react": "18.0.0",
    "react-ace": "10.0.0",
    "react-dom": "18.0.0",
    "recharts": "2.1.9",
    "set-interval-async": "2.0.3",
    "swr": "1.3.0",
    "tar": "6.1.11",
    "three": "0.139.2",
    "typedoc": "0.22.15",
    "typedoc-plugin-airthium": "ssh://git@github.com:Airthium/typedoc-plugin-airthium.git#1.0.4",
    "typescript": "4.6.3",
    "url-join": "4.0.1",
    "uuid": "8.3.2"
  },
  "packageManager": "yarn@3.2.0"
}
